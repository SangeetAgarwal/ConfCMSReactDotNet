{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AuthenticationConfiguration": {
    "JwtBearerConfiguration": {
      "Authority": "https://securetoken.google.com/<projectId>",
      "TokenValidationConfiguration": {
        "Issuer": "https://securetoken.google.com/<projectId>",
        "Audience": "<projectId>"
      }
    }
  }
}
